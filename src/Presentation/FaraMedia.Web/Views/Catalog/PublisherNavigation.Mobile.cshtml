@model IList<PublisherNavigationModel>
@using Fara.Core.Domain.Catalog
@using Fara.Core.Infrastructure
@using Fara.Services.Catalog
@using Fara.Web.Models.Catalog
@if (Model.Count > 0)
{<div class="block block-publisher-navigation">
    <div class="title">
        @T("Publishers")
    </div>
    <div class="clear">
    </div>
    <ul data-role="listview" data-inset="true" data-filter="false">
        @foreach (var publisher in Model)
        {
            <li><a href="@Url.RouteUrl("Publisher", new { publisherId = publisher.Id, SeName = publisher.SeName })">
                @publisher.Name</a></li>
        }
    </ul>
    </div>
}